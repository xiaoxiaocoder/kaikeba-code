{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.0.0-beta.6",
    "@ant-design/pro-layout": "^6.3.0",
    "@ant-design/pro-table": "^2.4.4",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.14",
    "axios": "^0.19.2",
    "lint-staged": "^10.0.7",
    "merge": "^1.2.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.14",
    "yorkie": "^2.0.0"
  }
}
